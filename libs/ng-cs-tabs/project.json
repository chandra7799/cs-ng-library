{
  "name": "ng-cs-tabs",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/ng-cs-tabs/src",
  "prefix": "ng-cs",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:package",
      "outputs": ["dist/libs/ng-cs-tabs"],
      "options": {
        "project": "libs/ng-cs-tabs/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/ng-cs-tabs/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/ng-cs-tabs/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/ng-cs-tabs"],
      "options": {
        "jestConfig": "libs/ng-cs-tabs/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/ng-cs-tabs/**/*.ts",
          "libs/ng-cs-tabs/**/*.html"
        ]
      }
    }
  },
  "tags": []
}
